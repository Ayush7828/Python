   "source": [
    "number=int(input(\"Enter a number \"))\n",
    "h=number%100\n",
    "t=(number/10)%10\n",
    "u=number%10\n",
    "s=u*u*u+t*t*t+h*h*h\n",
    "if number==s:\n",
    "    print(\"It is armstrong number \",number)\n",
    "else:\n",
    "    print(\"It is not a Armstrong Number \",number)"
